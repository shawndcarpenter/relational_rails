<%= form_with url: "/musicians/#{@musician.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :instument %>
  <%= form.text_field :instrument %>
  <%= form.label :position %>
  <%= form.text_field :position %>
  <%= form.label :years_active %>
  <%= form.text_field :years_active %>
  <%= form.submit 'Update Musician' %>
<% end %>