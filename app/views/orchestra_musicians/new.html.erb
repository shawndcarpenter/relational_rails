<%= form_with url: "/orchestras/#{@orchestra.id}/musicians", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :instument %>
  <%= form.text_field :instrument %>
  <%= form.label :position %>
  <%= form.text_field :position %>
  <%= form.label :years_active %>
  <%= form.text_field :years_active %>
  <%= form.label :on_leave %>
  <%= form.check_box :on_leave %>

  <%= form.submit "Create Musician" %>
<% end %>